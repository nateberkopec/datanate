# Terraform Variables for Cloudflare Infrastructure
# Copy this file to .env and fill in your values

# Cloudflare Configuration
TF_VAR_cloudflare_api_token=your-cloudflare-api-token-here
TF_VAR_cloudflare_account_id=your-cloudflare-account-id-here
TF_VAR_cloudflare_zone_id=your-cloudflare-zone-id-here

# GitHub Configuration
TF_VAR_github_username=your-github-username
TF_VAR_github_repo_name=datanate

# Optional: Custom Domain
TF_VAR_custom_domain=

# HTTP Basic Auth
TF_VAR_auth_username=admin
TF_VAR_auth_password=your-secure-password-here